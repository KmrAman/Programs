#include <bits/stdc++.h>
using namespace std;
int main() {
    string s;
    cin>>s;
    int k;
    cin>>k;
    int l=s.size();
    for(int i=0;i<l;i++){
        int n=((l+i)-k)%l;
        cout<<s[n];
    }
    cout<<endl;
    for(int i=0;i<l;i++){
        int m=((l+i)+k)%l;
        cout<<s[m];
    }
}
