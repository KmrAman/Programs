#include <iostream>
using namespace std;
struct Node{
    int data;
    Node* next;
    Node* prev;
}; 
struct Node* head;
void print();
void insert(int data,int n){
    Node* temp=new Node();
    temp->data=data;
    temp->prev=NULL;
    temp->next=NULL;
    if(n==1){
    temp->prev=NULL;
    temp->next=head;
    head=temp;
    return;
    }
    Node* temp1=head;
    
    for(int i=0;i<n-2;i++){
        temp1=temp1->next;
        
    }
    if(temp1->next!=NULL){
        temp1->next->prev=temp;
        temp->next=temp1->next;
        temp->prev=temp1;
        temp1->next=temp;
    }
    else{
        temp->next=NULL;
        temp->prev=temp1;
        temp1->next=temp;
    }
    
}
void print(){
    Node* temp=head;
    while(temp!=NULL){
        cout<<temp->data<<" ";
        temp=temp->next;
    }
}
void insertAtFront(int x){
    Node* temp=new Node();
    temp->data=x;
    temp->prev=NULL;
    temp->next=head;
    head =temp;struct Node* head;
    }

int main() {
    insertAtFront(7);
    insertAtFront(6);
    insertAtFront(4);
    print();
    cout<<endl;
    insert(3,3);
    print();
    return 0;
    }
