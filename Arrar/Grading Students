//https://www.hackerrank.com/challenges/grading/problem
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int i,t,n;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
        if(a[i]%5==3&&a[i]>37){
            a[i]=a[i]+2;
        }
        if(a[i]%5==4&&a[i]>37){
            a[i]=a[i]+1;
        }
       
    }
    for(i=0;i<n;i++){
        cout<<a[i]<<endl;
    }
    return 0;
}
