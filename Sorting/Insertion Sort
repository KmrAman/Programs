#include <iostream>
using namespace std;
void selectionsort(int a[],int n){
    int i,j;
    for(i=1;i<n;i++){
        int value=a[i];
        int hole=i;
        while(hole>0&&a[hole-1]>value){
            a[hole]=a[hole-1];
            hole=hole-1;
        }
        a[hole]=value;
    }
}
int main(){
 int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    selectionsort(a,n);
    for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }
}
