//https://www.hackerrank.com/challenges/compare-the-triplets/problem
#include <bits/stdc++.h>

using namespace std;
int main(){
    int i,s1=0,s2=0;
    int a[3];
    int b[3];
    for(i=0;i<3;i++){
        cin>>a[i];
    }
     for(i=0;i<3;i++){
        cin>>b[i];
    }
    for(i=0;i<3;i++){
        if(a[i]>b[i]){
            s1=s1+1;
        }
        if(a[i]<b[i]){
           s2=s2+1;
        }
    }
    cout<<s1<<" "<<s2;
    return 0;
}
